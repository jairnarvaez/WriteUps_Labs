{% load static %}

<!DOCTYPE html>
<html lang="es">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>WriteUps_Labs - Cyber Space Odyssey</title>
    <script src="https://unpkg.com/lucide@latest"></script>
	<link rel="stylesheet" href="{% static 'css/styles_main.css' %}">
	<link rel="stylesheet" href="{% static 'css/styles_header.css' %}">
	<link rel="stylesheet" href="{% static 'css/styles_sidebar.css' %}">
	<link rel="stylesheet" href="{% static 'css/styles_main_content.css' %}">
	<link rel="stylesheet" href="{% static 'css/styles_home_page.css' %}">
